         <fx:include fx:id="visualisationPanel" source="VisualisationPanel.fxml" />
      </AnchorPane>
      <Button fx:id="generateButton" layoutX="18.0" layoutY="80.0" mnemonicParsing="false" onAction="#generateVisualisation" prefHeight="27.0" prefWidth="87.0" text="Stwórz nową" />
